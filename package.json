{
  "name": "openaedmap-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "/",
  "type": "module",
  "dependencies": {
    "@maplibre/maplibre-gl-geocoder": "^1.5.0",
    "@mdi/js": "^7.3.67",
    "@mdi/react": "^1.6.1",
    "bulma": "^0.9.4",
    "bulma-checkradio": "^2.1.3",
    "events": "^3.3.0",
    "i18next": "^23.7.11",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "maplibre-gl": "^3.6.2",
    "opening_hours": "^3.8.0",
    "osm-auth": "^2.4.0",
    "react": "^18.1.0",
    "react-bulma-components": "^4.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^14.0.0",
    "react-image-gallery": "^1.3.0",
    "react-select": "^5.8.0",
    "store": "^2.0.12",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "sprites": "spritezero ./public/img/sprite ./src/marker_icons; spritezero --ratio=2 ./public/img/sprite@2x ./src/marker_icons; spritezero --ratio=4 ./public/img/sprite@4x ./src/marker_icons",
    "lint": "biome lint .",
    "lint-fix": "biome lint . --apply",
    "format": "biome format . --write",
    "check": "biome ci .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "css-build": "sass sass/mystyles.scss src/mystyles.css",
    "css-watch": "npm run css-build -- --watch"
  },
  "browserslist": {
    "production": [
      "defaults"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@beyondtracks/spritezero-cli": "^2.3.1",
    "@biomejs/biome": "1.4.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-image-gallery": "^1.2.4",
    "@types/store": "^2.0.5",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "sass": "^1.69.5",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  }
}
