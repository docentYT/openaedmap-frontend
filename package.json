{
  "name": "openaedmap-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "/",
  "dependencies": {
    "@mapbox/timespace": "^2.0.4",
    "@maplibre/maplibre-gl-geocoder": "^1.5.0",
    "@mdi/js": "^7.3.67",
    "@mdi/react": "^1.6.1",
    "bulma": "^0.9.4",
    "bulma-checkradio": "^2.1.3",
    "cross-env": "^7.0.3",
    "i18next": "^23.7.11",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "maplibre-gl": "^3.6.2",
    "opening_hours": "^3.8.0",
    "osm-auth": "^2.3.0",
    "react": "^18.1.0",
    "react-bulma-components": "^4.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^14.0.0",
    "react-image-gallery": "^1.3.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.8.0",
    "store": "^2.0.12",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "sprites": "spritezero ./public/img/sprite ./src/marker_icons; spritezero --ratio=2 ./public/img/sprite@2x ./src/marker_icons; spritezero --ratio=4 ./public/img/sprite@4x ./src/marker_icons",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix": "eslint . --ext .ts,.tsx --fix",
    "css-build": "sass sass/mystyles.scss src/mystyles.css",
    "css-watch": "npm run css-build -- --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@beyondtracks/spritezero-cli": "^2.3.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-image-gallery": "^1.2.4",
    "@types/store": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "sass": "^1.69.5"
  }
}
